<scenario>
  <name>Total Shopping Cart Test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>total-rules</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>0.0</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shippingTotal</name>
          <value>0.0</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>cartItemPromoSavings</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sc1</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>25.0</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shippingTotal</name>
          <value>2.99</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>cartItemPromoSavings</name>
          <value>3.0</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sc2</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Total Shopping Cart</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>cartTotal</fieldName>
          <expected>0.0</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>cartItemPromoSavings</fieldName>
          <expected>0.0</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc1</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>cartTotal</fieldName>
          <expected>27.990000000000002</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>cartItemPromoSavings</fieldName>
          <expected>-3.0</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc2</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.coolstore.ShoppingCart</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>