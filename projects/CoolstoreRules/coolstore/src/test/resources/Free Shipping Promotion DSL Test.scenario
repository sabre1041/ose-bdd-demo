<scenario>
  <name>Free Shipping Promotion DSL Test.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>promo-rules</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>75</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shippingTotal</name>
          <value>10.99</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sc1</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value>74.99</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shippingTotal</name>
          <value>10.99</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sc2</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Free Shipping Promotion DSL</ruleName>
      <expectedCount>1</expectedCount>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>0</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc1</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>shippingTotal</fieldName>
          <expected>10.99</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc2</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.coolstore.ShoppingCart</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>