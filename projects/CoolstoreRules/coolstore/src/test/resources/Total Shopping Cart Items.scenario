<scenario>
  <name>Total Shopping Cart Items.scenario</name>
  <maxRuleFirings>100000</maxRuleFirings>
  <globals/>
  <fixtures>
    <org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
      <name>pricing-rules</name>
    </org.drools.workbench.models.testscenarios.shared.ActivateRuleFlowGroup>
    <fact-data>
      <type>ShoppingCart</type>
      <fieldData>
        <field-data>
          <name>cartItemTotal</name>
          <value></value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>cartItemPromoSavings</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sc</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>ShoppingCartItem</type>
      <fieldData>
        <field-data>
          <name>price</name>
          <value>10.99</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>quantity</name>
          <value>1</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shoppingCart</name>
          <value>=$sc</value>
          <nature>2</nature>
        </field-data>
        <field-data>
          <name>promoSavings</name>
          <value></value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sci1</name>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>ShoppingCartItem</type>
      <fieldData>
        <field-data>
          <name>price</name>
          <value>5.00</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>quantity</name>
          <value>2</value>
          <nature>1</nature>
        </field-data>
        <field-data>
          <name>shoppingCart</name>
          <value>=$sc</value>
          <nature>2</nature>
        </field-data>
        <field-data>
          <name>promoSavings</name>
          <value>1.50</value>
          <nature>1</nature>
        </field-data>
      </fieldData>
      <name>$sci2</name>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-rule>
      <ruleName>Total Shopping Cart Items</ruleName>
      <expectedFire>true</expectedFire>
    </expect-rule>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>cartItemTotal</fieldName>
          <expected>20.990000000000002</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
        <expect-field>
          <fieldName>cartItemPromoSavings</fieldName>
          <expected>3.0</expected>
          <operator>==</operator>
          <nature>1</nature>
        </expect-field>
      </fieldValues>
      <name>$sc</name>
      <anonymous>false</anonymous>
    </expect-fact>
  </fixtures>
  <rules/>
  <inclusive>false</inclusive>
  <packageName></packageName>
  <imports>
    <imports>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.coolstore.ShoppingCart</type>
      </org.drools.workbench.models.datamodel.imports.Import>
      <org.drools.workbench.models.datamodel.imports.Import>
        <type>com.redhat.coolstore.ShoppingCartItem</type>
      </org.drools.workbench.models.datamodel.imports.Import>
    </imports>
  </imports>
</scenario>